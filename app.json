{
  "expo": {
    "name": "VELEA",
    "slug": "VeleaMembership",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/flashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#975542"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.velea.asia",
      "icon": "./assets/logo_ios.png",
      "requireFullScreen": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to take photos or scan QR codes.",
        "NSLocationWhenInUseUsageDescription": "Allow location access to show nearby services and improve your experience."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.velea.asia",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA"
      ],
      "config": {
        "usesNonExemptEncryption": false,
        "googleMaps": {
          "apiKey": "AIzaSyDj-sDv7p5f1bJ96pbqATFdSSBxQ_zahjM"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-asset",
      "expo-notifications",
      "expo-document-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "c72d7332-be9a-48b9-9625-7c4b29790c3e"
      }
    }
  }
}