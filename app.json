{
  "expo": {
    "name": "VELEA",
    "slug": "VeleaMembership",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/flashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#975542"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "kh.velea.com",
      "icon": "./assets/velea_ios.png",
      "requireFullScreen": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Our app need to use the camera to scan card membership qr"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/velea_andiord.png",
        "backgroundColor": "#ffffff"
      },
      "package": "kh.velea.com",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-asset",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "c72d7332-be9a-48b9-9625-7c4b29790c3e"
      }
    }
  }
}