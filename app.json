{
  "expo": {
    "name": "VELEA",
    "slug": "VeleaMembership",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/flashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#323232"
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": false,
      "bundleIdentifier": "kh.velea.com",
      "icon": "./assets/velea_ios.png",
      "requireFullScreen": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/velea_andiord.png",
        "backgroundColor": "#ffffff"
      },
      "package": "kh.velea.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "c72d7332-be9a-48b9-9625-7c4b29790c3e"
      }
    }
  }
}
